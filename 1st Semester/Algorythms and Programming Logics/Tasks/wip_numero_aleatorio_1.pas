	dec(numero);
	writeln(numero);  
	dec(numero);
	writeln(numero);  
	dec(numero);
	writeln(numero);  
	inc(numero);
	writeln(numero);  
	inc(numero);
	writeln(numero);  
	inc(numero);
	writeln(numero);  
	
	caractere := 'd';
	
	dec(caractere);
	writeln(caractere);
	dec(caractere);
	writeln(caractere);
	dec(caractere);
	writeln(caractere);
	inc(caractere);
	writeln(caractere);
	inc(caractere);
	writeln(caractere);
	inc(caractere);
	writeln(caractere);
	
End.